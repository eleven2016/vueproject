// 引用模板
var menuData = require('../assets/menu.json')

// 路由列表
var routes = []

// 将菜单数据加入到路由列表中
menuData.forEach((item) => {
  item.subMenu.forEach((menu) => {
    routes.push({
      path: `/${menu.id}`,
      name: menu.name,
      component: () => import(`@/components/${menu.id}`)
    })
  })
})

// 配置登录页面跳转路由
routes.push({
  path: `/`,
  name: 'home'
})

// 导出配置路由
export default routes
